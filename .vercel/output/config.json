{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		},
		{
			"src": "^\\/404\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/forgotpass\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/link\\/create\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/link\\/edit\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/me\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/s\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/signin\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/signup\\/?$",
			"dest": "_render"
		},
		{
			"src": "/.*",
			"dest": "_render",
			"status": 404
		}
	]
}